# ========================================
# CONFIGURACOES DO PROJETO ECONOMIA
# ========================================

# ========================================
# CONFIGURACOES GERAIS
# ========================================
TIMEZONE=America/Sao_Paulo
DOTNET_ENVIRONMENT=Development

# Build information (usado em CI/CD)
BUILD_NUMBER=dev-local
GIT_COMMIT=local-commit

# ========================================
# POSTGRESQL - CONFIGURACOES DE CONEXAO
# ========================================
POSTGRES__CREDENCIAIS__HOST=economia-postgresql
POSTGRES__CREDENCIAIS__PORT=5433
POSTGRES__CREDENCIAIS__USER=postgres
POSTGRES__CREDENCIAIS__PASSWORD=sua_senha_aqui

# ========================================
# POSTGRESQL - BANCO DE DADOS
# ========================================
POSTGRES__BANCO_DE_DADOS__ECONOMIA=economia
